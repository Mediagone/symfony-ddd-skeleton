{% extends 'Shared/Views/BaseLayout.twig' %}

{% register once '/static/theme/graindashboard/css/graindashboard.css' %}
{% register once '/static/theme/graindashboard/js/graindashboard.js' %}
{% register once '/static/theme/graindashboard/js/graindashboard.vendor.js' %}

{% register once 'fixed-width' in 'bodyClasses' %}


{% block BODY_CONTENT %}

    {% if HEADER is defined %}
        {% include 'Shared/Views/Components/PageHeader.twig' %}
    {% endif %}
    
    <main class="main">

        {% if MENU != null %}
            {% include 'Frontend/Views/Components/FrontendMenu.twig' %}
        {% endif %}
        
        <div class="content">
            {% block PAGE_CONTENT %}{% endblock %}
            {% if FOOTER is defined %}
                {% include 'Shared/Views/Components/PageFooter.twig' %}
            {% endif %}
        </div>
    </main>
    
    {#% include "Shared/Elements/NotificationPopups.twig" with {Domain: 'frontend'} %#}
{% endblock %}
