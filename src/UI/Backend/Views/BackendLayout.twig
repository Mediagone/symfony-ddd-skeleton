{% extends 'Shared/Views/Layout.twig' %}

{% register once '/static/jquery-3.5.1.min.js' priority 0 %}
{% register once '/static/theme/adminlte-2.4.18/vendors/font-awesome/css/font-awesome.css' %}
{% register once '/static/theme/adminlte-2.4.18/vendors/bootstrap/css/bootstrap.css' %}
{% register once '/static/theme/adminlte-2.4.18/vendors/bootstrap/js/bootstrap.js' %}
{% register once '/static/theme/adminlte-2.4.18/css/adminlte.css' %}
{% register once '/static/theme/adminlte-2.4.18/css/skins/skin-black-light.css' %}
{% register once '/static/theme/adminlte-2.4.18/js/adminlte.js' %}

{% register once 'fixed' in 'bodyClasses' %}
{% register once 'skin-black-light' in 'bodyClasses' %}
{% register once 'hold-transition' in 'bodyClasses' %}
{% register once 'sidebar-mini' in 'bodyClasses' %}


{% block BODY_CONTENT %}

    {% if HEADER != null %}
        {% include 'Shared/Views/Components/PageHeader.twig' %}
    {% endif %}
    
    <main class="main">
        
        {% if MENU != null %}
            {% include 'Shared/Views/Components/PageMenu.twig' %}
        {% endif %}
        
        <div class="content">
            {% block PAGE_CONTENT %}{% endblock %}
        </div>
    </main>
    
    {#% include "Shared/Elements/NotificationPopups.twig" with {Domain: 'backend'} %#}
{% endblock %}
