{% extends 'Shared/Views/BaseLayout.twig' %}

{% expect nullable 'App\\UI\\Shared\\Views\\Components\\PageHeader' as HEADER %}

{% register once '/static/theme/graindashboard/css/graindashboard.css' %}
{% register once '/static/theme/graindashboard/js/graindashboard.js' %}
{% register once '/static/theme/graindashboard/js/graindashboard.vendor.js' %}

{#% register once 'has-sidebar' in 'bodyClasses' %#}


{% block BODY_CONTENT %}

    {% if HEADER != null %}
        {% include 'Shared/Views/Components/PageHeader.twig' %}
    {% endif %}
    
    <main class="main">
        
        {% if MENU != null %}
            {% include 'Shared/Views/Components/PageMenu.twig' %}
        {% endif %}
        
        <div class="content">
            {% block PAGE_CONTENT %}{% endblock %}
        </div>
    </main>
    
    {#% include "Shared/Elements/NotificationPopups.twig" with {Domain: 'backend'} %#}
{% endblock %}
