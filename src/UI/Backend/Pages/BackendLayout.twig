{% extends 'Shared/Views/Layout.twig' %}

{% register once '/static/jquery-3.5.1.min.js' priority 0 %}
{% register once '/static/styles.css' %}

{% register once 'text-gray-500' in 'bodyClasses' %}


{% block HEAD_TITLE %}{% block PAGE_TITLE %}{% endblock %}{{ parent() }}{% endblock %}


{% block BODY_CONTENT %}

    <header class="page__header">
        <div class="page__main-menu px-2">
            
            {% include('Shared/Views/Components/Generic/Nav.twig') with { NAV: new('App\\UI\\Shared\\Views\\Components\\Generic\\Nav',
                [
                    { label:'Dashboard', href:path('backend_dashboard') },
                    { label:'Team', href:path('backend_account_list') },
                    { label:'Projects', href:'' },
                    { label:'Calendar', href:path('backend_page_list') },
                    { label:'Reports', href:path('backend_page_list') },
                ],
                {
                    attrClass:'',
                    submenuAttrClass:'',
                }
            )} only %}
            
            {% include('Shared/Views/Components/Generic/Nav.twig') with { NAV: new('App\\UI\\Shared\\Views\\Components\\Generic\\Nav',
                [
                    {
                        label:'<svg class="h-5 w-5" focusable="false" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 496 512"><path fill="currentColor" d="M248 8C111 8 0 119 0 256s111 248 248 248 248-111 248-248S385 8 248 8zm128 421.6c-35.9 26.5-80.1 42.4-128 42.4s-92.1-15.9-128-42.4V416c0-35.3 28.7-64 64-64 11.1 0 27.5 11.4 64 11.4 36.6 0 52.8-11.4 64-11.4 35.3 0 64 28.7 64 64v13.6zm30.6-27.5c-6.8-46.4-46.3-82.1-94.6-82.1-20.5 0-30.4 11.4-64 11.4S204.6 320 184 320c-48.3 0-87.8 35.7-94.6 82.1C53.9 363.6 32 312.4 32 256c0-119.1 96.9-216 216-216s216 96.9 216 216c0 56.4-21.9 107.6-57.4 146.1zM248 120c-48.6 0-88 39.4-88 88s39.4 88 88 88 88-39.4 88-88-39.4-88-88-88zm0 144c-30.9 0-56-25.1-56-56s25.1-56 56-56 56 25.1 56 56-25.1 56-56 56z" class=""></path></svg><span>' ~ app.user.account.Forename.toString ~ ' ' ~ app.user.account.Lastname.toString ~ '</span>',
                        items:[
                            {label:'Profile'},
                            {label:'Settings'},
                            {label:'<svg class="h-5 w-5" focusable="false" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 512 512"><path fill="currentColor" d="M400 54.1c63 45 104 118.6 104 201.9 0 136.8-110.8 247.7-247.5 248C120 504.3 8.2 393 8 256.4 7.9 173.1 48.9 99.3 111.8 54.2c11.7-8.3 28-4.8 35 7.7L162.6 90c5.9 10.5 3.1 23.8-6.6 31-41.5 30.8-68 79.6-68 134.9-.1 92.3 74.5 168.1 168 168.1 91.6 0 168.6-74.2 168-169.1-.3-51.8-24.7-101.8-68.1-134-9.7-7.2-12.4-20.5-6.5-30.9l15.8-28.1c7-12.4 23.2-16.1 34.8-7.8zM296 264V24c0-13.3-10.7-24-24-24h-32c-13.3 0-24 10.7-24 24v240c0 13.3 10.7 24 24 24h32c13.3 0 24-10.7 24-24z"></path></svg><span>'~('login.sign_out'|trans)~'</span>', href:path('frontend_security_logout'),
                                
                            },
                        ],
                    },
                ],
                {
                    attrClass:'',
                    submenuAttrClass:'right-0 min-w-48',
                }
            )} only %}
            
        </div>
    </header>


    <div class="flex flex-row h-full">
        <aside class="sidebar flex-none w-60 shadow fixed">
            {% include('Shared/Views/Components/Generic/Nav.twig') with { NAV: new('App\\UI\\Shared\\Views\\Components\\Generic\\Nav',
