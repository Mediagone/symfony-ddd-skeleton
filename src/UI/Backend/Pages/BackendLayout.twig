                        href:path('backend_dashboard'),
                        badges:[
                            {label:''~random(2,3), attrClass:'bg-green-500'},
                            {label:''~random(1,2), attrClass:'bg-yellow-500'}
                        ],
                        items:[
                            {label:'Item 2', href:path('backend_dashboard'), items:[
                                {label:'Sub-item 1'},
                                {label:'Sub-item 2', href:path('backend_dashboard'), badges:[{label:''~random(1,3), attrClass:'bg-red-500'}]},
                            ]},
                        ],
                    },
                ],
                {
                    attrClass:'',
                    submenuAttrClass:'',
                }
            )} only %}
        </aside>
        
        <main class="main flex-auto bg-gray-100 ml-60">
            <div class="overflow-scroll w-full h-full px-9">
            {% block PAGE_CONTENT %}{% endblock %}
            </div>
        </main>
        
        {% include 'Shared/Views/Components/Layout/Notifications.twig' with { DOMAIN:'backend' } only %}
    </div>
    
{% endblock %}
